#!/bin/sh
LOADER="/home/root/xovi/exthome/qt-resource-rebuilder/_ingatellent.qmd"
QMD_FILE="ingatellent/scrollScreenUpOrDown.qmd"

if ! grep -q "LOAD $QMD_FILE" "$LOADER" 2>/dev/null; then
    if [ ! -s "$LOADER" ]; then
        echo "LOAD $QMD_FILE" > "$LOADER"
    else
        sed -i "1i LOAD $QMD_FILE" "$LOADER"
    fi
fi
